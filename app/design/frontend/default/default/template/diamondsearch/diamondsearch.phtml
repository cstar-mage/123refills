<script type="text/javascript">

  function resizeSearch(){
    var curr_width = window.innerWidth ? window.innerWidth : $(document).width();
    if (curr_width < 992 ) {
      $("#search-form").css('width', '75px');
      $("#search-form input.t").css('width', '55px');
      $("#search-form button").css('display', 'none');
      $('#search-form input.t').focus(function() {
        if (curr_width < 992 ) {
          $("#search-form input.t").animate({width: '100px'}, "slow");
          $("#search-form").animate({width: '192px'}, "slow");
          $("#search-form button").fadeIn("slow");
        }
      });
      $('#search-form input.t').blur(function() {
        search_blur = setTimeout(function() {
          if(Sys.ie){
            if ($("#search-form input.t").val() == "" || $("#search-form input.t").val() == "SEARCH") {
              $("#search-form input.t").animate({width: '55px'}, "slow");
              $("#search-form button").fadeOut("slow");
              $("#search-form input.t").parent().animate({width: '75px'}, "slow");
            }
          } else {
            if ($("#search-form input.t").val() == "") {
              $("#search-form input.t").animate({width: '55px'}, "slow");
              $("#search-form button").fadeOut("slow");
              $("#search-form input.t").parent().animate({width: '75px'}, "slow");
            }
          }
        }, 200);
      });
      $('#search_icon').click(function() {
        $("#search-form input.t").focus();
      });
    } else {
      $("#search-form").css('width', '192px');
      $("#search-form input.t").css('width', '100px');
      $("#search-form button").css('display', 'block');
    }
  }
  if ('onorientationchange' in window) {
      $(window).on('orientationchange', function() {
        resizeSearch();
      });
    } else {
      $(window).on('resize', function() {
        resizeSearch();
      });
    }
    resizeSearch();


