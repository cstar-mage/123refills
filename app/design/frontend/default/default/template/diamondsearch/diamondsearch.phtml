<script>
  // Update custom reviews.
    function getReviews() {
      var reviewWords = $('.pr-snippet-review-count').html();
      if ( !reviewWords ) { return false; }

      var noReviewRep = /no/gim;
      if (noReviewRep.test(reviewWords)) {
        return '0';
      }

      var reviewCountRep = /\d+/gim;
      var res = reviewCountRep.exec(reviewWords);
      if (res) { return res[0]; }
    }

    function writeReviews() {
      var reviewCount = getReviews();
      if (reviewCount) {
        $('a[href=#profile] em').html('(' + reviewCount + ')');
      }
    }

    $(".row.explanation").live("DOMSubtreeModified", function() {
      writeReviews();
    });
  </script> 
