<?xml version="1.0" encoding="UTF-8"?>
<config>
    <sections>
        <general translate="label" module="livechatadmin">
            <tab>general</tab>
            <groups>
                <livechatadmin translate="label" module="livechatadmin">
                    <label>Admin Live Chat Options</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>0</show_in_website>
                    <show_in_store>0</show_in_store>
                    <fields>
                        <created_at translate="label">
                            <label>Created at date</label>
                            <frontend_type>text</frontend_type>
 							<frontend_model>livechatadmin/adminhtml_system_config_date</frontend_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                        </created_at>
                        <company_id translate="label">
                            <label>Company ID</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>21</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                        </company_id>
                    </fields>
                </livechatadmin>
            </groups>
        </general>
    </sections>
</config>